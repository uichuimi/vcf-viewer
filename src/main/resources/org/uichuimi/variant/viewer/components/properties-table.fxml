<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<BorderPane xmlns="http://javafx.com/javafx"
			xmlns:fx="http://javafx.com/fxml"
			fx:controller="org.uichuimi.variant.viewer.components.PropertiesTable"
>
	<center>
		<TableView fx:id="infoTable" prefHeight="9999">
			<columns>
				<TableColumn fx:id="info" style="-fx-font-style: italic" text="ID"/>
				<TableColumn fx:id="value" text="VALUE"/>
			</columns>
		</TableView>
	</center>
</BorderPane>
